void Calibrate()
{


  TCanavs *mycan1 = (TCanvas*)gROOT->FindObject("mycan1");
  if(!mycan)
    {
      mycan1 = new TCanvas("mycan1","",800,800);
      mycan1->Divide(4,4,0.0000001,0.0000001);
    }

  float energy1[2][32] = {0.};
  float energy2[2][32] = {0.};
